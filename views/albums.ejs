<h1>Albums</h1>
<% albums.forEach(album => { %>
    <%= album.name %>
    <% if (album.images[0]) { %>
        <img src="<%= album.images[0].url %>" alt="<%= album.name %>" />
        <% } else {%>
            <p>Image not available</p>
        <% } %>
        <a href="/tracks/<%- album.id %>">View Tracks</a>
<% }) %>